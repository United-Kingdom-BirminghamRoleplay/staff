{
  "commands": [
    {
      "command": ":warn",
      "usage": ":warn [player] [reason]",
      "permission": "Trial Moderator+",
      "description": "Issue warning for FailRP violations"
    },
    {
      "command": ":kick", 
      "usage": ":kick [player] [reason]",
      "permission": "Trial Moderator+",
      "description": "Remove player from ERLC server"
    },
    {
      "command": ":ban",
      "usage": ":ban [player] [time] [reason]", 
      "permission": "Moderator+",
      "description": "Temporarily ban player from server"
    },
    {
      "command": ":pban",
      "usage": ":pban [player] [reason]",
      "permission": "Administrator+", 
      "description": "Permanently ban player from ERLC"
    },
    {
      "command": ":tp",
      "usage": ":tp [player1] [player2]",
      "permission": "Support Staff+",
      "description": "Teleport players for assistance"
    },
    {
      "command": ":bring",
      "usage": ":bring [player]",
      "permission": "Moderator+",
      "description": "Bring player to your location"
    },
    {
      "command": ":goto",
      "usage": ":goto [player]", 
      "permission": "Support Staff+",
      "description": "Teleport to player's location"
    },
    {
      "command": ":spectate",
      "usage": ":spectate [player]",
      "permission": "Moderator+",
      "description": "Spectate player for monitoring"
    },
    {
      "command": ":logs",
      "usage": ":logs [player]",
      "permission": "Moderator+", 
      "description": "View player's punishment history"
    },
    {
      "command": ":announce",
      "usage": ":announce [message]",
      "permission": "Administrator+",
      "description": "Server-wide announcement"
    }
  ]
}